{
    "monitoredValues": [{
        "name": "cpu",
        "interval": 5000
    }, {
        "name": "ram",
        "interval": 10000
    }, {
        "name": "hdd",
        "interval": 45000,
        "params": {
            "paths": ["/"]
        }
    }, {
        "name": "container",
        "interval": 30000
    }, {
        "name": "network",
        "interval": 15000,
        "params": {
            "interfaces": ["eth0", "tun0"]
        }
    }, {
        "name": "software",
        "interval": 60000
    }, {
        "name": "uptime",
        "interval": 60000
    }],
    "redis": {
        "host": "localhost",
        "port": 6379,
        "socket_keepalive": true
    }
}